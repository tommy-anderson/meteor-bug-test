FROM vm/ubuntu:18.04
CHECKPOINT disabled
RUN echo GIT_COMMIT_TITLE
RUN echo GIT_COMMIT_TITLE =~ ([skip-deploy]|[no-deploy]|[skip-release]|[no-release]) && matched
RUN echo hi
SKIP REMAINING IF API_EXTRA=~ ^$
SKIP REMAINING IF GIT_COMMIT_TITLE =~ ([skip-deploy]|[no-deploy]|[skip-release]|[no-release])
RUN echo here
RUN echo here