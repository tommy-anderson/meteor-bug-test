{
  "name": "tic-tac-toe",
  "private": true,
  "scripts": {
    "wait-on-test": "wait-on http://localhost:4000 --httpTimeout 2400000",
    "wait-on-dev": "wait-on http://localhost:3000 --httpTimeout 2400000",
    "start-dev": "meteor run --port 3000",
    "start-test": "meteor run --port 4000",
    "test": "meteor test --once --driver-package meteortesting:mocha",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "visualize": "meteor --production --extra-packages bundle-visualizer"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.3",
    "chai": "^4.3.4",
    "is-odd": "^3.0.1",
    "meteor-node-stubs": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "wait-on": "^6.0.0"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.jsx",
      "server": "server/main.js"
    }
  }
}
